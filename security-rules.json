{
  "rules": {
    ".read": true,
      "Teams": {
        "$team_id": {
          "Pairs": {
            ".indexOn": "active"
          },
          "Persons": {
            ".indexOn": "active",
            "first": {
              ".validate": "newData.isString() && newData.val().matches(/[A-Za-z]+/)"
            },
            "last": {
              ".validate": "newData.isString() &&newData.val().matches(/[A-Za-z]+/)"
            }
          }
        },
        ".write": true
      }
  }
}
