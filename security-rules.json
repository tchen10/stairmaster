{
  "rules": {
    ".read": true,
    "Teams": {
      "$team_id": {
        "Pairs": {
          ".indexOn": "active"
        },
        "Persons": {
          ".indexOn": "active",
          "first": {
            ".validate": "newData.isString() && newData.val().length > 1"
          },
          "last": {
            ".validate": "newData.isString() && newData.val().length > 1"
          }
        }
      },
      ".write": true
    }
  }
}
